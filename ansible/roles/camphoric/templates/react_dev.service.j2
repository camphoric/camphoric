[Unit]
Description=react_dev daemon
After=network.target
Requires=react_dev.socket

[Service]
User={{ camphoric_user }}
Group={{ camphoric_user }}
WorkingDirectory=/home/{{ camphoric_user }}/camphoric/client

ExecStart=/usr/bin/node run python \
          manage.py runserver 0.0.0.0:8000

[Install]
WantedBy=multi-user.target
